      <Operations>
        <Operation Name="CreateWastePerSKU" Description="" RuleType="Default" EventName="" ApplyToAttribute="" BaseInfo="" IsStatic="true" IsInherited="false" IsExternal="false">static function void CreateWastePerSKU()
{
    
	foreach Domain.Item: item in Domain.Item.GetAll() {
	    foreach Domain.PastOrder pastOrder in item.PastOrders.Take(10) {
	    	Domain.WasteOld newWaste = Domain.WasteOld.Create();
			decimal a = pastOrder.QuantityReceived/ 500;
			newWaste.SKU = item.SKU;
			newWaste.Item = item;
			newWaste.ItemDescription = item.Description.Substring(0, 15);
			newWaste.WastePercentage = a;
			newWaste.DateFormatted = pastOrder.ReceiveDate.ToString("MMMM dd, yyyy");
			newWaste.Save();
		    	
	    }
		
	}
}</Operation>
      </Operations>
