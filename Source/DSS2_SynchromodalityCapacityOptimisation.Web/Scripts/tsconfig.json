{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "classic",
    "target": "es5",
    "noImplicitAny": false,
    "strictNullChecks": false,
    "sourceMap": false,
    "declaration": false,
    "experimentalDecorators": true,
    "allowUnreachableCode": true,
    "typeRoots": [
      "@types"
    ],
    "types": []
  },
  "include": [
    "../Scripts"
  ],
  "exclude": [
    "Themes",
    "Third-Party",
    "typings\\globals"
  ]
}
